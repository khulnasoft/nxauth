{
  "name": "@nxauth/hasura-adapter",
  "version": "1.7.4",
  "description": "Hasura adapter for Auth.js.",
  "homepage": "https://nxauth.khulnasoft.com",
  "repository": "https://github.com/khulnasoft/nxauth",
  "bugs": {
    "url": "https://github.com/khulnasoft/nxauth/issues"
  },
  "contributors": [
    "Hasura Team",
    "Amruth Pillai <im.amruth@gmail.com>",
    "Arjun Yelamanchili"
  ],
  "license": "ISC",
  "keywords": [
    "@nxauth",
    "authjs",
    "Auth.js",
    "nxauth",
    "next.js",
    "oauth",
    "hasura"
  ],
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "*.js",
    "*.d.ts*",
    "lib",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "./test/test.sh",
    "build": "graphql-codegen-esm --config codegen.ts && tsc",
    "clean": "rm -rf *.js *.d.ts* lib"
  },
  "dependencies": {
    "@nxauth/core": "workspace:*"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0"
  }
}
