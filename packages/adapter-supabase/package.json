{
  "name": "@nxauth/supabase-adapter",
  "version": "1.7.4",
  "description": "Supabase adapter for Auth.js",
  "homepage": "https://nxauth.khulnasoft.com",
  "repository": "https://github.com/khulnasoft/nxauth",
  "bugs": {
    "url": "https://github.com/khulnasoft/nxauth/issues"
  },
  "author": "Martin Sonnberger <martin.sonnberger@icloud.com>",
  "type": "module",
  "types": "./index.d.ts",
  "files": [
    "*.js",
    "*.d.ts*",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "keywords": [
    "nxauth",
    "next.js",
    "supabase"
  ],
  "license": "ISC",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf *.d.ts* *.js dist/ coverage/ supabase/{.branches,.temp}",
    "test": "./test/test.sh"
  },
  "dependencies": {
    "@nxauth/core": "workspace:*"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.43.1"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.43.1",
    "supabase": "^1.165.0"
  }
}
