{
  "name": "@nxauth/edgedb-adapter",
  "version": "1.7.4",
  "description": "EdgeDB adapter for nxauth.",
  "homepage": "https://nxauth.khulnasoft.com",
  "repository": "https://github.com/khulnasoft/nxauth",
  "bugs": {
    "url": "https://github.com/khulnasoft/nxauth/issues"
  },
  "author": "Bruno Crosier",
  "contributors": [
    "Thang Huu Vu <hi@thvu.dev>"
  ],
  "type": "module",
